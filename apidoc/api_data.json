    "url": "/newsurvey",
    "title": "Crear una encuesta",
    "name": "newSurvey",
    "group": "ENCUESTAS",
    "version": "0.1.0",
    "description": "<p>Su función es crear una nueva encuesta.</p>",
    "permission": [
